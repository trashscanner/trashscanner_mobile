{
  "expo": {
    "name": "TrashScanner",
    "slug": "trashscanner",
    "scheme": "trashscanner",
    "version": "0.1.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.trashscanner.mobile",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": ["expo-asset", "expo-font"]
  }
}
